#include<stdio.h>
int main()
{
    int n,rev=0,a,rem;
    printf("Enter a number");
    scanf("%d",&n);
    a=n;
    while(n>0)
    {
        rem=n%10;
        rev=rev*10+rem;
        n=n/10;
    }
    if(rev==a)
    printf("%d is a Palindrome number",a);
    else
    printf("%d is not a Palindrome number",a);
    return 0;
}
