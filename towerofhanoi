#include<stdio.h>
int main()
{
    int n;
    char beg='A',aux='B',end='C';
    printf("Enter the no. of discs");
    scanf("%d",&n);
    toh(n,beg,aux,end);
    return 0;
}
void toh(int n,char beg,char aux,char end)
{
    if(n==1)
    printf("Move from %c to %c\n",beg,end);
    else
    {
        toh(n-1,beg,end,aux);
        toh(1,beg,aux,end);
        toh(n-1,aux,beg,end);
    }
}
