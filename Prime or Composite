#include<stdio.h>
int main()
{
    int a,i,f=0;
    printf("Enter a number");
    scanf("%d",&a);
    for(i=2;i<a;i++)
    {
        if(a%i==0)
        {
            f=1;
            break;
        }
    }    
    if(f==1)
    {printf("%d is the composite number",a);}
    else
    {printf("%d is a prime number",a);}
    return 0;
}
